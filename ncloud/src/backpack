<!--视频弹窗-->
function open_v(){
        $('#video_player').stop().fadeIn();
		var video = document.getElementById("myVideo");
		video.volume=0;
		video.pause();
    }
function close_v(){
        $('#video_player').stop().hide();
		var video = document.getElementById("myVideo");
		video.currentTime = 0;
		video.volume=0;
		video.pause();	
    }
<!--背景图片点击消失-->
$(function(){
	$(".sliderNav_left a").click(function(){
		$(".sliderNav_left").css("background","none");
		});
	});

function changPage(){
		var thisPage =$(".cd-slider-navigation .selected");
		if(thisPage.next().length!=0){
			thisPage.next().click();
		}else{
			$(".cd-slider-navigation").find("li:eq(0)").click();
		}
		setTimeout('changPage()',10000);
	}
changPage();
</script>
