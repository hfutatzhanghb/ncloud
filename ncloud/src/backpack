<script src="${basePath}/resources/common/website/js/main.js" ></script> <!-- Resource jQuery -->
<script>
$(function(){
	$('#dowebok').fullpage({
		'navigation': true,
		afterLoad: function(anchorLink, index){
			if(index == 1){
				$('.section2').find('#video_close').click();
			}
			if(index == 2){
			    $('.section3').find('.btnGroup1').delay(0).animate({
					opacity:'1'
				}, 10000, 'easeOutExpo');
				$('.section3').find('.btnGroup2').delay(500).animate({
					opacity:'1'
				}, 10000, 'easeOutExpo');
				$('.section3').find('.cir1').addClass('cir1-move');
				$('.section3').find('.cir2').addClass('cir2-move');
				$('.section2').find('#video_close').click();
			}
			if(index == 3){
				$('.section5').find('.lef').delay(0).animate({
					width: '690px', opacity:'1'
				}, 1500, 'easeOutExpo');
				$('.section5').find('.rigt').delay(250).animate({
					left: '0', opacity:'1'
				}, 1500, 'easeOutExpo');
			}
			
		},
		
		onLeave: function(index, direction){
			
			if(index == '2'){
			$('.section3').find('.btnGroup1').delay(0).animate({
					opacity:'0'
				}, 1500, 'easeOutExpo');
			$('.section3').find('.btnGroup2').delay(0).animate({
					opacity:'0'
				}, 1500, 'easeOutExpo');
            $('.section3').find('.cir1').removeClass('cir1-move');
			$('.section3').find('.cir2').removeClass('cir2-move');
			}
			
		}
	});
});

</script>
