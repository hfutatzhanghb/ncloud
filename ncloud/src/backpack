<script>
// tab标签切换
    jQuery.jqtab = function(tabtit,tab_conbox,shijian) {
        $(tab_conbox).find(".tab_con").hide();
        $(tabtit).find("li:first").addClass("blue").show();
        $(tab_conbox).find(".tab_con:first").show();

        $(tabtit).find("li").bind(shijian,function(){
          $(this).addClass("blue").siblings("li").removeClass("blue");
            var activeindex = $(tabtit).find("li").index(this);
            $(tab_conbox).children().eq(activeindex).fadeIn(200).siblings().hide();
           return false;
        });
    };
// tab标签切换
  

	// tab大气泡标签切换2
    jQuery.jqtab = function(tabtit,tabtit2,tab_conbox,shijian) {
        $(tab_conbox).find(".tab_con").hide();
        $(tab_conbox).find(".tab_con:first").show();
		$(tabtit2).find("li:first").addClass("blue").show();

        $(tabtit).find("li").bind(shijian,function(){
            var activeindex = $(tabtit).find("li").index(this);
            $(tab_conbox).children().eq(activeindex).fadeIn(20).siblings().hide();
			 $(tabtit2).children().eq(activeindex).addClass("blue").siblings("li").removeClass("blue");
           return false;
        });
    };

/*顶部导航切换	*/
$(function() {
	var $t, leftX, newWidth;
	$('.menu ul').append('<div class="block"></div>');
	var $block = $('.block');

	$block.width($(".current").width()).css('left', $('.current a').position().left).data('rightLeft', $block.position().left).data('rightWidth', $block.width());

	$('.menu ul li').find('a').hover(function() {
		$t = $(this);
		leftX = $t.position().left;
		newWidth = $t.parent().width();

		$block.stop().animate({
			left: leftX,
			width: newWidth
		},300);
	}, function() {
		$block.stop().animate({
			left: $block.data('rightLeft'),
			width: $block.data('rightWidth')
		},300)
	})
	// $($('.menu ul li.current'))[0].setAttribute('id','firstLi');
	// var getClass = $($('.menu ul li.current'))[0].getAttribute('class','current');
	// console.log(getClass)
})
/*客服悬浮效果*/
$(function(){
	//Sldie
	$(".kefu").mouseenter(function(){
		$(".kefulist").fadeIn();
	});
	$(".kefu").mouseleave(function(){
		$(".kefulist").hide();
	});
});
